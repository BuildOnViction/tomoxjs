{
    "parserOptions": {},
    "extends": [
        "standard"
    ],
    "plugins": [ ],
    "rules": {
        "key-spacing"          : 0,
        "jsx-quotes"           : [2, "prefer-single"],
        "max-len"              : [2, 750, 2],
        "object-curly-spacing" : [2, "always"],
        "promise/catch-or-return": "warn",
        "indent": ["warn", 4]
    }
}
